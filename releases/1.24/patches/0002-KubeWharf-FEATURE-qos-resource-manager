+	}
+	l = len(m.PodName)
+	if l > 0 {
+		n += 1 + l + sovApi(uint64(l))
+	}
+	l = len(m.ContainerName)
+	if l > 0 {
+		n += 1 + l + sovApi(uint64(l))
+	}
+	if m.ContainerType != 0 {
+		n += 1 + sovApi(uint64(m.ContainerType))
+	}
+	if m.ContainerIndex != 0 {
+		n += 1 + sovApi(uint64(m.ContainerIndex))
+	}
