+	DaemonsetKind                  = "DaemonSet"
+)
diff --git a/pkg/kubelet/cm/qosresourcemanager/endpoint.go b/pkg/kubelet/cm/qosresourcemanager/endpoint.go
new file mode 100644
index 00000000000..7ff2067e8a9
